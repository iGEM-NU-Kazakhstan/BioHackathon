{
  "name": "project_el_nino",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "declarativeContent",
    "storage",
    "activeTab",
    "tabs"
  ],
  "background": {
    "scripts": [
      "scripts/background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html"
  }
  
}
